<p id="notice"><%= notice %></p>



<section class="notes-index">
  <div class="notes-wrapper">

    <% @notes.each do |note| %>
      <%=link_to note, :action => 'go' do%>
        <div id="img-note">
          <h2 class="note-title">     <%= note.title %> </h2>
        </div>
      <%end%>
    <% end %>
    
  </div>
</section>

<%=link_to new_note_path, :action => 'go' do%>
    <div class="links-new-note2">
        +
    </div>
<%end%>


